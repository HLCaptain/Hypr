<UserControl
    x:Class="HyprWinUI3.Views.CustomControls.EditorTreeControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Custom="using:Microsoft.UI.Xaml.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:HyprWinUI3.Views.CustomControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="300"
    d:DesignWidth="400"
    mc:Ignorable="d">

    <Grid>
        <Custom:TreeView
            Name="treeView"
            Collapsed="treeView_Collapsed"
            Expanding="treeView_Expanding"
            ItemInvoked="treeView_ItemInvoked">
            <Custom:TreeView.ItemTemplate>
                <DataTemplate x:DataType="TreeViewNode">
                    <TextBlock Text="wow" />
                </DataTemplate>
            </Custom:TreeView.ItemTemplate>
        </Custom:TreeView>
    </Grid>
</UserControl>
